{
  "name": "clickmerce",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8051",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check --ignore-path .prettierignore .",
    "prettier:fix": "prettier --write --ignore-path .prettierignore .",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:status": "npx supabase status",
    "db:link": "cross-env dotenv -e .env.local -- supabase link --project-ref $NEXT_PUBLIC_SUPABASE_PROJECT_REF",
    "db:migration": "npx supabase migration up --linked --include-all",
    "db:reset": "npx supabase db reset --linked",
    "db:types": "node -e \"require('fs').mkdirSync('supabase/types', {recursive: true})\" && npx supabase gen types typescript --linked --schema public > supabase/types/database.types.ts",
    "db:setup": "yarn db:migration && yarn db:types",
    "setup": "yarn && yarn db:start && yarn db:setup"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.4",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.0",
    "@tanstack/react-query": "^5.22.2",
    "antd": "^5.15.0",
    "antd-img-crop": "^4.25.0",
    "axios": "^1.6.7",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.11",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lottie-react": "^2.4.0",
    "next": "14.0.4",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.13.0",
    "react-phone-input-2": "^2.15.1",
    "tailwind-merge": "^2.2.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-unused-imports": "^3.0.0",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "supabase": "^2.34.3",
    "tailwindcss": "^3.3.0",
    "typescript": "5.3.3"
  }
}
